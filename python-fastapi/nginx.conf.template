events {}

http {
    server {
        listen ${RUNTIME_PORT};

        location = /specialize {
            proxy_pass http://127.0.0.1:9999/specialize;
        }

        location = /v2/specialize {
            proxy_pass http://127.0.0.1:9999/v2/specialize;
        }

        location / {
            proxy_pass http://127.0.0.1:8080/;
        }
    }
}
